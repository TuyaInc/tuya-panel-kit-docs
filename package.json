{
  "private": true,
  "name": "tuya-panel-kit-docs",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "job": "ts-node scripts/render-static"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@nodelib/fs.walk": "^1.2.8",
    "@tuya/dumi-theme-tuya": "^2.1.1",
    "babel-plugin-import": "^1.13.3",
    "cheerio": "^1.0.0-rc.10",
    "dumi": "^1.1.16",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "speech-rule-engine": "3.3.1"
  }
}
